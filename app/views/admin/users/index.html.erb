<h1>Dashboard</h1>

<h2>Users</h2>
<ul>
  <% @users.each do |user| %>
    <li><%= user.email %></li>
  <% end %>
</ul>

<h2>Random Pairs</h2>
<ul>
  <% @pairs.each do |(home, guest)| %>
    <li>
      <% if guest %>
        <strong><%= home.email %></strong> vs <strong><%= guest.email %></strong>
      <% else %>
        <%= home.email %>
      <% end %>
    </li>
  <% end %>
</ul>
